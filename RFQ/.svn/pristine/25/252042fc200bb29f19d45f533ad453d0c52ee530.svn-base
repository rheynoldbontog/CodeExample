<!-- Buyer RFQ Line Modal -->
<div id="buyerRFQLineModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="buyerRFQLineModalLabel" aria-hidden="true" style="height: 70%;">
  <div class="modal-header default-primary-color color3">
    <div class="btn-group pull-right">
                                    <a class="btn btn-success dropdown-toggle" data-toggle="dropdown" href="#">
                                        <span>General Form</span>
                                        <span class="caret"></span>
                                    </a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a href="#">
                                                <span>General Form</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <span>Test Equipment & Rentals and Calibration Form</span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
    <h4 id="myModalLabel">Edit RFQ Line</h4>
  </div>
  <div class="modal-body" style="max-height: calc(100vh - 350px); overflow-y: scroll;">
    <div class="row-fluid">
      <div class="span12 master-wrapper-center-1">
          <ul class="nav nav-tabs" id="myTab">
              <li class="active"><a href="#rfqLineDetails" data-toggle="tab">RFQ Line Details</a></li>
              <li><a href="#quotations" data-toggle="tab">Quotation</a></li>
          </ul>

          <div class="tab-content">
              <div class="tab-pane active" id="rfqLineDetails">
                  <form class="form-horizontal">
                <input type="hidden" data-bind="value: vm.modal.Id"/>

                <div class="row">
                    <div class="span12">
                        <div class="span6">
                        <h4 style="margin-left: 30px;">General Form</h4>
                        <div class="control-group">
                            <label class="span3 control-label rfo-label">RFQ Line No:</label>
                            <div class="span9 controls">
                                <input type="text" class="rfo-medium-textbox"/>
                            </div>
                        </div>
                        <div class="control-group" data-bind="validationElement: vm.modal.itemDescription">
                            <label class="span3 control-label rfo-label">Item Description:</label>
                            <div class="span9 controls">
                                <textarea class="rfo-medium-textbox" rows="3"></textarea>
                            </div>
                        </div>
                        <div class="control-group" data-bind="validationElement: vm.modal.quantity">
                            <label class="span3 control-label rfo-label">Quantity:</label>
                            <div class="span9 controls">
                                <input type="text" class="rfo-medium-textbox"/>
                            </div>
                        </div>
                        <div class="control-group" data-bind="validationElement: vm.modal.quantityUnit">
                            <label class="span3 control-label rfo-label">UOM:</label>
                            <div class="span9 controls">
                                <input type="text" class="rfo-medium-textbox"/>
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="span3 control-label rfo-label">Maker:</label>
                            <div class="span9 controls">
                                <input type="text" class="rfo-medium-textbox"/>
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="span3 control-label rfo-label">MakerPN:</label>
                            <div class="span9 controls">
                                <input type="text" class="rfo-medium-textbox"/>
                            </div>
