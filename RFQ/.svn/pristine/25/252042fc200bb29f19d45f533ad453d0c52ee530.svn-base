        <div class="hero-unit">
        <h1>MRO RFQ Tool</h1>
        <p></p>
        @if (_workContext.CurrentUser.Username == null)
        { 
            <p>
                <a id="login" class="btn btn-primary btn-large" href="javascript:void(0)">Log me in
                    &rsaquo;&rsaquo;</a>
            </p>
        }
        else
        {
            if (ViewBag.IsRequestor)
            {
                <p>
                    <a id="createRFQ" class="btn btn-primary btn-large" href="javascript:void(0)">Create RFQ
                    &rsaquo;&rsaquo;</a>
                </p>
            }
            
        }
    </div>
    @if (_workContext.CurrentUser.Username != null)
    {
      <div class="row-fluid">
        <form class="form-horizontal">
            <div class="span6 master-wrapper-center-1">
                <h3>Search RFQ By:</h3>
                <div class="control-group">
                    <label class="control-label">RFQ No.</label>
                    <div class="controls">
                        <input type="text" placeholder="RFQ No." />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">Commodity</label>
                    <div class="controls">
                        <input type="text" placeholder="Commodity" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">Status</label>
                    <div class="controls">
                        <div class="btn-group">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                <span>All</span>
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu" role="menu">
                                <li>
                                    <a href="#">
                                        <span>Open</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span>Close</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span>All</span>
                                    </a>
                                </li>
                           </ul>
                 </div>
                    </div>
                </div>
            </div>
            <div class="span6 master-wrapper-center-1">
                <h3 style="color: White;">Search RFQ By:</h3>
                <div class="control-group">
                    <label class="control-label">Requester</label>
                    <div class="controls">
                        <input type="text" placeholder="Requester" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">Buyer</label>
                    <div class="controls">
                        <input type="text" placeholder="Buyer" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">Search</label>
                    <div class="controls">
                        <input type="text" placeholder="Search" />
                    </div>
                </div>
            </div>
        </form>
      </div>
    }
</div>

