/* HTML GENERAL */
body,h1,h2,h3,h4,h5,h6,address,blockquote,dd,dl,hr,form,input{margin:0;padding:0;}
body{font-family:arial, verdana, helvetica, sans-serif;font-size:12px;color:#f93;background-color:#ffffff;background:#4459FF url(images/bg-body.gif) repeat-x;}
a{color:#3a3a3a;text-decoration:none;}
a:hover{color:#f93;text-decoration:none;}
img{border:0;}
h1,h2,h3,h4,h5,h6{font-family:arial, helvetica, sans-serif;}
h2,h3{font-size:16px;}
textarea{width:100%;font-family:Verdana, Arial, sans-serif;font-size:11px;}
checkbox,input,radio,select,button{font-size:11px;margin:4px;}
input[type="text"]{font-size:13px;padding:4px;border:solid 1px #404040;-webkit-border-radius: 3px;-moz-border-radius: 3px; border-radius: 3px;}
input[type="password"]{font-size:13px;padding:4px;border:solid 1px #404040;-webkit-border-radius: 3px;-moz-border-radius: 3px; border-radius: 3px; }
select{font-size:13px;padding:3px;border:solid 1px #404040;-webkit-border-radius: 3px;-moz-border-radius: 3px; border-radius: 3px; }
checkbox{margin:10px;}
label{font-size:13px;padding:4px;margin-top:10px;}
fieldset{margin:0px 0px 15px 5px;border:1px solid #c9c9c9;padding:15px;}
legend{font-weight:bold;line-height:20px;font-size:12px;}

