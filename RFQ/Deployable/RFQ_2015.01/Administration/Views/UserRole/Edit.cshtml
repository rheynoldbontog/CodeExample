@model UserRoleModel
@{
    //page title
    ViewBag.Title = @T("Admin.Users.UserRoles.EditUserRoleDetails").Text;
}
@using Telerik.Web.Mvc.UI
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-users.png")" alt="" />
            @T("Admin.Users.UserRoles.EditUserRoleDetails") - @Model.Name @Html.ActionLink("(" + T("Admin.Users.UserRoles.BackToList") + ")", "List")
        </div>
        <div class="options">
            <input type="submit" name="save" class="t-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="t-button" value="@T("Admin.Common.SaveContinue")" />
            <span id="userrole-delete" class="t-button">@T("Admin.Common.Delete")</span>
        </div>
    </div>
    @Html.Partial("_CreateOrUpdate", Model)
}
@Html.DeleteConfirmation("userrole-delete")
