@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";
    ViewBag.Title = " Access Denied";
}

<div class="hero-unit pull-center">
    <h1>
        @ViewBag.Title <small><font face="Tahoma" color="red"></font></small></h1>
    <br />
    <h2>@ViewBag.ErrorMessage</h2>
    
    <p>Use your browsers <b>Back</b> button to navigate to the page you have prevously come from</p>
    <p><b>Or you could just press this neat little button:</b></p>
    <a href="#" class="btn btn-medium btn-info" onclick="location.href='@Url.Action("Index","Home")'"><i class="icon-home icon-white"></i>Take Me Home</a>
</div>
